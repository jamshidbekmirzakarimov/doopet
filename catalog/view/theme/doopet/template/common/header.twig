<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}">
<!--<![endif]-->

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <title>{{ title }}</title>
  <base href="{{ base }}" />
  {% if description %}
  <meta name="description" content="{{ description }}" />
  {% endif %} {% if keywords %}
  <meta name="keywords" content="{{ keywords }}" />
  {% endif %}
  <script src="catalog/view/javascript/jquery/jquery-3.7.0.min.js" type="text/javascript"></script>
  <link href="catalog/view/javascript/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen" />
  <script src="catalog/view/javascript/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
  <link href="catalog/view/javascript/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
  <link href="//fonts.googleapis.com/css?family=Open+Sans:400,400i,500,600,300,700" rel="stylesheet" type="text/css" />
  <link href="catalog/view/theme/doopet/stylesheet/stylesheet.css" rel="stylesheet" />
  {% for style in styles %}
  <link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
  {% endfor %} {% for script in scripts %}
  <script src="{{ script }}" type="text/javascript"></script>
  {% endfor %}
  <script src="catalog/view/javascript/common.js" type="text/javascript"></script>
  <script src="catalog/view/javascript/script.js" type="text/javascript"></script>
  {% for link in links %}
  <link href="{{ link.href }}" rel="{{ link.rel }}" />
  {% endfor %} {% for analytic in analytics %}
  {{
  analytic
  }}
  {% endfor %}
</head>

<body>
  <!-- Header mobile start -->
  <div class="header-mobile">
    <div class="header-search">
      {{search}}
    </div>
    <div class="header-mobile-close">
      <img src="catalog/view/theme/doopet/image/close-btn.svg" alt="close">
    </div>
    <div class="container">
    <div class="header-mobile-info">
      <div id="top-links" class="nav">
        <ul class="list-inline">
          <li>
            <i class="fa fa-location-arrow"></i>
            <span> {{ address }}</span>
          </li>
          <li>
            <a href="mailto:{{ email }}">
              <i class="fa fa-envelope"></i>
              <span>{{ email }}</span>
            </a>
          </li>
          <li>
            <a href="{{ contact }}">
              <i class="fa fa-phone"></i>
              <span>{{ telephone }}</span>
            </a>

          </li>
          <li class="dropdown">
            <a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown"><i
                class="fa fa-user"></i>
              <span>{{
                text_account
                }}</span>
              <span class="caret"></span></a>
            <ul class="dropdown-menu dropdown-menu-right">
              {% if logged %}
              <li>
                <a href="{{ account }}">{{ text_account }}</a>
              </li>
              <li>
                <a href="{{ order }}">{{ text_order }}</a>
              </li>
              <li>
                <a href="{{ transaction }}">{{ text_transaction }}</a>
              </li>
              <li>
                <a href="{{ download }}">{{ text_download }}</a>
              </li>
              <li>
                <a href="{{ logout }}">{{ text_logout }}</a>
              </li>
              {% else %}
              <li>
                <a href="{{ register }}">{{ text_register }}</a>
              </li>
              <li>
                <a href="{{ login }}">{{ text_login }}</a>
              </li>
              {% endif %}
            </ul>
          </li>
          <li>
            <a href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}"><i class="fa fa-heart"></i>
              <span>{{
                text_wishlist
                }}</span></a>
          </li>
        </ul>
      </div>
    </div>
    <ul class="header-menu">
      {% if informations %} {% for information in informations %}
      <li>
        <a href="{{ information.href }}">{{ information.title }}</a>
      </li>
      {% endfor %} {% endif %}
      <li><a href="blog-categ ory">{{blog}}</a></li>
      <li><a href="{{ contact }}">{{ text_contact }}</a></li>
    </ul>
    </div>
  </div>
  <!-- Header mobile end -->

  <nav id="top">
    <div class="container">
      <div id="top-links" class="nav pull-right">
        <ul class="list-inline">
          <li>
            <i class="fa fa-location-arrow"></i>
            <span> {{ address }}</span>
          </li>
          <li>
            <a href="mailto:{{ email }}">
              <i class="fa fa-envelope"></i>
              <span>{{ email }}</span>
            </a>
          </li>
          <li>
            <a href="{{ contact }}">
              <i class="fa fa-phone"></i>
              <span>{{ telephone }}</span>
            </a>

          </li>
          <li class="dropdown">
            <a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown"><i
                class="fa fa-user"></i>
              <span>{{
                text_account
                }}</span>
              <span class="caret"></span></a>
            <ul class="dropdown-menu dropdown-menu-right">
              {% if logged %}
              <li>
                <a href="{{ account }}">{{ text_account }}</a>
              </li>
              <li>
                <a href="{{ order }}">{{ text_order }}</a>
              </li>
              <li>
                <a href="{{ transaction }}">{{ text_transaction }}</a>
              </li>
              <li>
                <a href="{{ download }}">{{ text_download }}</a>
              </li>
              <li>
                <a href="{{ logout }}">{{ text_logout }}</a>
              </li>
              {% else %}
              <li>
                <a href="{{ register }}">{{ text_register }}</a>
              </li>
              <li>
                <a href="{{ login }}">{{ text_login }}</a>
              </li>
              {% endif %}
            </ul>
          </li>
          <li>
            <a href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}"><i class="fa fa-heart"></i>
              <span>{{
                text_wishlist
                }}</span></a>
          </li>
        </ul>
      </div>
    </div>
  </nav>

  <header>
    <div class="header-center">
      <div class="container">
        <div class="row">
          <div class="col-xs-2">
            <div id="logo">
              {% if logo %}<a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}"
                  class="img-responsive" /></a>{% else %}
              <h1>
                <a href="{{ home }}">{{ name }}</a>
              </h1>
              {% endif %}
            </div>
          </div>
          <div class="col-sm-10">
            <ul class="header-menu">
              {% if informations %} {% for information in informations %}
              <li>
                <a href="{{ information.href }}">{{ information.title }}</a>
              </li>
              {% endfor %} {% endif %}
              <li><a href="blog-categ ory">{{blog}}</a></li>
              <li><a href="{{ contact }}">{{ text_contact }}</a></li>
            </ul>
            <div class="hamburger">
              <img src="catalog/view/theme/doopet/image/hamburger.svg" alt="hamburder">
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="header-bottom">
      <div class="container">
        <div class="header-bottom-row">
          <div class="catalog-btn">
            <span><img src="catalog/view/theme/doopet/image/hamburger.svg" alt="hamburger"></span>
            <span>Каталог</span>
            {{ menu }}
          </div>
          <div class="header-search">
            {{search}}
          </div>
          <div class="header-cart">
            {{cart}}
          </div>
        </div>
      </div>
    </div>
  </header>
</body>

</html>